{
  "name": "IranJiran",
  "description": "Remove unnecessary(='いらん') parts from JIRA board.",
  "manifest_version": 3,
  "version": "0.1.0",
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.atlassian.net/jira/software/projects/*"],
      "js": ["src/content.ts"]
    }
  ],
  "permissions": [
    "storage"
  ]
}
